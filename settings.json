{
  "editor.fontFamily": "Hack, Menlo, monospace",
  "editor.fontSize": 17,
  "editor.fontLigatures": false,
  "editor.lineHeight": 0,
  "editor.tabSize": 2,
  "editor.minimap.enabled": true,
  "editor.formatOnPaste": true,
